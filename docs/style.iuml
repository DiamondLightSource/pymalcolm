skinparam handwritten true

skinparam defaultFontName Humor Sans

skinparam frame {
  FontSize 14
  FontStyle plain
}

skinparam component {
  FontSize 12
}

skinparam state {
  BackgroundColor #99CFCF
  BackgroundColor<<Rest>> #AAE7AA
  BackgroundColor<<Abort>> #FFBE89
  BackgroundColor<<Fault>> #F03232
  BackgroundColor<<Disabled>> #AAAAAA
  BorderColor Gray
  ArrowColor Black
  FontSize 14
  ArrowFontSize 12
  AttributeFontSize 12
}

skinparam backgroundColor transparent
